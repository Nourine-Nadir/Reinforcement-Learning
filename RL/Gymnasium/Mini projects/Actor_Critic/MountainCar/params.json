    "actor_lr": 5e-5,
    "critic_lr": 1e-4,
    "max_memory_size": 0,
    "model_path": "saved_models/ActorCritic_model",
    "agent_path": "saved_models/agent_model",
    "layer1_nodes": 256,
    "layer2_nodes": 256,
    "layer3_nodes": -1,
    "update_freq": 4
  }
}